PATH_TO_ENV_FILE="{{ PATH_TO_ENV_FILE }}"
TOKEN_LOCATION_BOT="{{ TOKEN_LOCATION_BOT }}"
APP_NAME="{{ app_name }}"
API_ID="{{ API_ID }}"
API_HASH="{{ API_HASH }}"
REST_API={% if REST_API is defined %}"{{ REST_API }}"{% else %}"not-using"{% endif %}

REST_API_TOKEN={% if REST_API_TOKEN is defined %}"{{ REST_API_TOKEN }}"{% else %}"not-using"{% endif %}

PARAMS={% if PARAMS is defined %}'{{ PARAMS }}'{% else %}'{"placeholder": "not-using"}'{% endif %}

LOG_TO_FILE='yes'
LOG_DIR=/var/log/{{ app_name }}
